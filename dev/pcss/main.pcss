@charset "utf-8";

@import "_reset.pcss";
@import "_vars.pcss";
@import "_mixins.pcss";

/* Initial
-----------------------------------------------------------------------------*/

body {
	background: #fff;
	color: $default-color;
	font: 15px/20px $default-font;
}
a {
	transition: all $transition-duration;

	&,
	&:active {
		color: $link-color;
		text-decoration: none;
	}
	&:hover {
		color: $link-color--hover;
	}
}
input, select, textarea, button {
	color: $default-color;
	font-family: $default-font;
}

/* Common
-----------------------------------------------------------------------------*/

.inner {
	lost-center: calc($column-size * 12 + $gutter-size * 11 + 20px * 2) 20px;

	@media (--palm) {
		padding-right: 13px;
		padding-left: @padding-right;
	}
}
.masthead {
	padding-top: 116px;
	padding-bottom: 64px;
	background: #f3f6f8 url("img/masthead-bg.jpg") 50% 50% no-repeat;
	background-size: cover;
	text-align: center;

	@media (--palm) {
		padding-top: 82px;
		padding-bottom: 0;
		background-image: url("img/masthead-bg--palm.jpg");
	}

	&__logo {
		margin-bottom: 42px;

		@media (--palm) {
			margin-bottom: 18px;
		}
	}
	&__slogan {
		margin-bottom: 15px;
		font-weight: bold;
		font-size: 48px;
		line-height: 56px;

		@media (--palm) {
			margin-bottom: 9px;
			font-size: 26px;
			line-height: 32px;
		}
	}
	&__slogan-breaker {
		@media (--palm) {
			display: none;
		}
	}
	&__subslogan {
		margin-bottom: 34px;
		font-size: 20px;
		line-height: 30px;

		@media (--palm) {
			font-size: 18px;
			line-height: 24px;
		}
	}
	&__action-wrapper {
		margin-bottom: 56px;

		@media (--palm) {
			display: none;
		}
	}
	&__keyboard-wrapper {
		@media (--palm) {
			position: relative;

			&:before {
				position: absolute;
				top: 50%;
				right: -10px;
				bottom: 0;
				left: -10px;
				display: block;
				background: #fff;
				content: "";
			}
		}
	}
	&__keyboard {
		max-width: 100%;
		border-radius: 14px;
		box-shadow: 0 4px 13px rgba(#000, .08);

		@media (--palm) {
			position: relative;
			border-radius: 6px;
			box-shadow: 0 2px 5px rgba(#000, .66);
		}
	}
}
.logo {
	&__image {
		@media (--palm) {
			width: 154px;
		}
	}
}

.header {
	position: absolute;
	left: 0;
	right: 0;
	top: 0;
	z-index: 3;

	&__i {
		display: flex;
		flex-direction: row;
		flex-wrap: wrap;
		position: relative;
		z-index: 12;
		justify-content: space-between;
		align-items: center;
		height: 65px;
		padding: 0 14px;
		border-bottom: 1px solid #ecf0f1;

		.--opened & {
			border-bottom: 1px solid rgba(80, 80, 80, 0.1);
		}

		@media (--not-palm) {
			height: 116px;
			padding: 0 50px;
		}
	}

	&__logo {
		&__image {
			width: 164px;

			@media (--not-palm) {
				width: 184px;
			}
		}

		@media (--not-palm) {
			position: relative;
			z-index: 2;
		}
	}

	&__menu {
		visibility: hidden;
		position: absolute;
		left: 0;
		right: 0;
		top: 0;
		z-index: -1;
		padding: 87px 12px 22px;
		opacity: 0;
		background: #fff;
		transition: opacity .3s;

		&.--desktop {
			display: none;
		}

		.--opened & {
			visibility: visible;
			opacity: 1;
			z-index: 11;
		}

		&__item {
			display: block;
			padding: 10px 0;
			font-size: 18px;
			line-height: 18px;
			font-weight: 500;
			color: #000;

			&.--active {
				color: $link-color--active;
			}

			@media (--not-palm) {
				padding: 15px 30px 11px;
			}

			@media (--desk) {
				padding: 15px 48px 11px;
			}
		}

		@media (--not-palm) {
			visibility: visible;
			z-index: 1;
			opacity: 1;
			left: 200px;
			right: 200px;
			padding-top: 35px;
			text-align: center;
			background: none;

			&.--desktop {
				display: flex;
				flex-direction: row;
				justify-content: center;
			}

			&.--mobile {
				display: none;
			}

			&__item {
				font-size: 15px;
			}
		}

		@media (--between-palm-and-desk) {
			position: static;
			padding-top: 18px;
		}
	}

  &__contact {
	display: none;

	@media (--not-palm) {
	  display: block;
	  position: relative;
	  z-index: 2;
	  font-weight: 500;
	  color: $link-color;
	  cursor: pointer;

	  &__icon {
		position: relative;
		top: 2px;
		margin-right: 8px;
	  }
	}
  }

  @media (--not-palm) {
	&__i {
		border: none;
	}
  }
}

.header__menu-btn {
  position: relative;
  width: 30px;
  height: 30px;
  right: -3px;

  &:before, &:after, &__i {
	content: '';
	display: block;
	position: absolute;
	left: 4px;
	right: 3px;
	height: 2px;
	border-radius: 1px;
	background: #000;
	transform: rotate(0deg);
	transition: transform .15s;
  }

  &:before {
	top: 7px;
  }

  &:after {
	bottom: 9px;
  }

  &__i {
	top: 13px;

	.--opened & {
	  background: transparent;
	}
  }

  .--opened & {
	z-index: 12;

	&:before {
	  top: 12px;
	  transform: rotate(-45deg);
	}

	&:after {
	  bottom: 16px;
	  transform: rotate(45deg);
	}
  }

  @media (--not-palm) {
	display: none;
  }
}

.breadcrumbs {
	padding-top: 82px;
	padding-bottom: 8px;
	font-size: 16px;
	line-height: 24px;

	&__link {
		font-weight: bold;
		color: #000;
	}

	&__current {
		color: #7f8c8d;
	}

	&__arrow {
		margin-right: 10px;
		margin-left: 10px;
	}

	@media (--not-palm) {
		padding-top: 116px;
	}
}

.users {
	padding: 30px 0;
	border-bottom: 1px solid #ecf0f1;

	&__inner {
		display: flex;
		flex-flow: row wrap;
		align-items: center;
		justify-content: center;
	}
	&__title {
		padding: 10px 5px;
		color: #7F8C8D;
		text-align: center;
		text-transform: uppercase;
		letter-spacing: 1.81px;
		font-weight: normal;
		font-size: 10px;
		line-height: 17px;
	}
	&__list {
		display: flex;
		flex-flow: row wrap;
		align-items: center;
		justify-content: space-evenly;
	}
	&__item {
		padding: 10px 25px;

	  	@media (--palm) {
		  	padding: 10px 15px;
		}
	}
	&__logo {
	  	@media (--palm) {
			&.--n1 {
			  width: 58px;
			}
			&.--n2 {
			  width: 60px;
			}
			&.--n3 {
			  width: 75px;
			}
			&.--n4 {
			  width: 38px;
			}
			&.--n5 {
			  width: 24px;
			}
			&.--n6 {
			  width: 75px;
			}
	  }
	}
}
.products {
	@media (--not-palm) {
		lost-flex-container: row;
		margin-bottom: 60px;
		margin-top: 70px;


		&__tabs {
			order: 1;
			lost-column: 7/12;
			padding-left: calc($column-size);
		}
	}

	&__tab-titles {
		lost-flex-container: row;
		justify-content: center;
		margin-top: 20px;
		margin-bottom: 36px;

		@media (--palm) {
			margin-top: 15px;
			margin-bottom: 21px;
		}
	}

	&__tab {
		display: none;

		&--current {
			display: block;
		}

		&__img {
			width: 100%;
		}

		@media (--palm) {
			margin-left: -13px;
			margin-right: -13px;
		}
	}
}
.tab-card {
	width: 92px;
	border: 2px solid #0057fb;
	font-size: 16px;
	line-height: 30px;
	text-align: center;
	color: #0057fb;
	transition-timing-function: ease-out;
	transition-duration: .15s;
	transition-property: background, color;

	&:hover {
		cursor: pointer;
	}

	&:first-child {
		border-radius: 5px 0 0 5px;
	}

	&:last-child {
		border-radius: 0 5px 5px 0;
	}

	&--current {
		position: relative;
		background: #0057fb;
		color: #fff;
	}

	@media (--palm) {
		width: 50%;
	}
}

.features {
	&__title {
		margin-bottom: 23px;
		font-size: 26px;
		line-height: 32px;
		text-align: center;
	}

	@media (--not-palm) {
		order: 0;
		lost-column: 5/12;
		margin-bottom: 72px;

		&__title {
			margin-bottom: 35px;
			padding-top: 13px;
			font-size: 36px;
			line-height: 46px;
		}
	}

	&__list {
		margin: 0 auto;
		max-width: calc($column-size * 6 + $gutter-size * 5);
	}
	&__item {
		padding-left: calc($column-size + $gutter-size);
		min-height: height("img/features__item--accelerate.svg");
		background-repeat: no-repeat;

		&:not(:last-child) {
			margin-bottom: 36px;
		}
		&--accelerate {
			background-image: svg-load("img/features__item--accelerate.svg");
		}
		&--discover {
			background-image: svg-load("img/features__item--discover.svg");
		}
		&--improve {
			background-image: svg-load("img/features__item--improve.svg");
		}

		@media (--palm) {
			padding-left: 88px;

			&:not(:last-child) {
				margin-bottom: 25px;
			}

			&--accelerate {
				background-size: 58px 56px;
				background-position: 8px 0;
			}
			&--discover {
				background-size: 57px 56px;
				background-position: 8px 0;
			}
			&--improve {
				background-size: 62px 16px;
				background-position: 4px 10px;
			}
		}
	}
	&__feature-name {
		margin-bottom: 7px;
		font-weight: bold;
		font-size: 26px;
		line-height: 1;

		@media (--palm) {
			margin-bottom: 4px;
			font-size: 18px;
			line-height: 24px;
		}
	}
	&__description {
		font-size: 16px;
		line-height: 22px;

		@media (--palm) {
			font-size: 15px;
			line-height: 20px;
		}
	}
}
.twitter-wrapper {
	margin-bottom: 40px;
}
.twitter {
	display: block;
	height: 80px;
	border: 2px solid;
	border-radius: 5px;

	@media (--palm) {
		height: 50px;
	}

	&:hover {
		border-color: $link-color--hover;
		background: $link-color--hover;
		color: #fff;
	}
	&__inner {
		position: relative;
		display: block;
		margin: auto;
		padding-left: calc($column-size + $gutter-size - 2px);
		max-width: calc($column-size * 6 + $gutter-size * 5);
		height: 100%;

		@media (--palm) {
			padding-left: 55px;
			max-width: initial;
		}
	}
	&__logo {
		position: absolute;
		top: 50%;
		left: 28px;
		margin-top: calc(height("img/svg-sprite/twitter__logo.svg") / -2);

		@media (--palm) {
			left: 18px;
			margin-top: calc(@height / -2);
			width: 25px;
			height: 20px;
		}
	}
	&__handle {
		display: block;
		padding-top: 18px;
		font-weight: bold;
		font-size: 24px;
		line-height: 1;

		@media (--palm) {
			padding-top: 0;
			font-size: 18px;
			line-height: 48px;
		}
	}
	&__description {
		display: block;
		font-size: 15px;
		line-height: 24px;

		@media (--palm) {
			display: none;
		}
	}
	&__arrow {
		position: absolute;
		top: 50%;
		right: 32px;
		margin-top: calc(height("img/svg-sprite/twitter__arrow.svg") / -2);

		@media (--palm) {
			right: 12px;
			margin-top: calc(@height / -2);
			width: 22px;
			height: 20px;
		}
	}
}
.photos-and-buy {
	margin-bottom: 24px;

	@media (--lap-and-bigger) {
		lost-flex-container: row;
	}
}
.photos {
	@media (--desk) {
		lost-column: 8/12 flex;
	}
	@media (--lap) {
		lost-column: 7/12 flex;
	}
	@media (--smaller-than-lap) {
		margin-bottom: 30px;
	}
	@media (--palm) {
		margin-right: -10px;
		margin-bottom: 23px;
		margin-left: @margin-right;
	}
}
.buy {
	&__title {
		margin-top: 15px;
		margin-bottom: 11px;
		font-size: 24px;
		line-height: 32px;
	}
	&__price {
		font-size: 36px;
		line-height: 44px;
	}
	&__shipping {
		margin-bottom: 13px;
		font-size: 16px;
		line-height: 24px;
		font-weight: bold;
	}
	&__shopify {
		margin-bottom: 10px;
	}
	&__security {
		position: relative;
		margin-bottom: 25px;
		padding-top: 1px;
		padding-bottom: 1px;
		padding-left: 23px;
		background: svg-load("img/buy__security.svg") 0 2px no-repeat;
		color: #7f8c8d;
		font-size: 13px;
		line-height: 16px;
	}
	&__sizes {
		margin-bottom: 30px;

		&__title {
			margin-bottom: 8px;
			font-size: 16px;
			line-height: 24px;
		}

		&__sections {
			border: 1px solid #d1dbdc;
			border-radius: 5px;
		}

		&__section {
			display: flex;
			align-items: stretch;

			&:last-of-type {
				border-top: 1px solid #d1dbdc;
			}
		}

		&__list {
			padding: 14px 0 14px 18px;

			&__item {
				padding-left: 15px;
				position: relative;
				font-size: 14px;
				line-height: 22px;

				&:before {
					content: '';
					display: block;
					position: absolute;
					left: 0;
					top: 8px;
					width: 4px;
					height: 4px;
					border-radius: 3px;
					background: rgba(0,0,0,0.3);
				}
			}
		}

		&__size {
			display: flex;
			align-items: center;
			justify-content: space-around;
			flex: 0 0 48px;
			border-right: 1px solid #d1dbdc;
			font-size: 16px;
			line-height: 16px;
			font-weight: bold;
			text-align: center;
			background: #ecf0f1;
		}
	}

	@media (--not-palm) {
		&__title {
			margin-bottom: 30px;
			font-size: 36px;
			line-height: 46px;
		}

		&__price {
			font-size: 36px;
			line-height: 46px;
		}

		&__security {
			margin-bottom: 40px;
		}

		&__sizes {
			width: 320px;
		}

		&__size {
			&__title {
				font-size: 18px;
				line-height: 22px;
			}
		}
	}

	@media (--desk) {
		&__title {
			margin-top: 0;
		}
	}
}

.qualities {
	margin-bottom: 15px;

	&__item {
		margin-bottom: 17px;
		position: relative;
		padding-left: 62px;

		&__icon {
			position: absolute;

			&.--n1 {
				width: 43px;
				height: 28px;
				top: 6px;
				left: 0;
			}
			&.--n2 {
				width: 30px;
				height: 42px;
				top: 2px;
				left: 8px;
			}
			&.--n3 {
				width: 38px;
				height: 38px;
				top: 2px;
				left: 0;
			}
		}

		&__title {
			margin-bottom: 2px;
			font-size: 18px;
			line-height: 22px;
		}

		&__descr {
			font-size: 15px;
			line-height: 20px;
		}
	}

	@media (--lap-and-bigger) {
		display: flex;
		padding: 37px 0 35px;
		margin-bottom: 35px;
		border-top: 1px solid #ecf0f1;
		border-bottom: 1px solid #ecf0f1;

		&__item {
			flex: 0 0 33%;
			&:not(:last-child) {
				padding-right: 30px;
			}
		}
	}
}
.buy-features {
	&__feature {
		padding-left: 62px;
		background-repeat: no-repeat;

		&:not(:last-child) {
			margin-bottom: 19px;
		}
		&--gift {
			background-image: svg-load("img/buy-features__feature--gift.svg");
			background-position: 0 3px;
		}
		&--produced {
			background-image: svg-load("img/buy-features__feature--produced.svg");
			background-position: 8px 3px;
		}
	}
	&__name {
		font-weight: bold;
		font-size: 18px;
		line-height: 22px;
	}
	&__description {
		a {
			color: currentColor;
			text-decoration: underline;

			&:hover {
				color: $link-color--hover;
			}
		}
	}
	&__link {
	}
}
.tweets {
	&__i {
		margin-right: -13px;
	}

	&__title {
		margin-bottom: 29px;
		font-weight: bold;
		font-size: 36px;
		line-height: 46px;
		text-align: center;

		@media (--palm) {
			margin-bottom: 14px;
			font-size: 24px;
			line-height: 32px;
			text-align: left;
		}
	}
	&__list {
		margin-bottom: 40px;

		@media (--not-palm) {
			margin-bottom: 80px;
		}
	}
	&__tweet {
		padding-right: 13px;
		padding-top: 4px;

		@media (--not-palm) {
			display: inline-block;
			vertical-align: top;
			width: 49%;
			padding-right: 22px;
			padding-bottom: 22px;
		}
		@media (--desk) {
			width: 33%;
			padding-right: 22px;
			padding-bottom: 22px;
		}
	}

	@media (--not-palm) {
		margin-right: -22px;
	}

	.twitter-tweet-rendered {
		margin: 0 !important;
	}
}

.instagram-media, .twitter-tweet {
	min-width: 260px !important;
	margin: 0 !important;
}

.product-links {
	padding: 36px 0 38px;

	&__i {
		lost-flex-container: row;
	}

	&__title {
		margin-bottom: 17px;
		text-align: center;
		font-size: 26px;
		line-height: 32px;
	}

	&__item {
		lost-column: 1/1;
		lost-flex-container: row;
		position: relative;
		height: 110px;
		margin-bottom: 8px;
		align-items: center;
		border-radius: 5px;
		background: #ecf0f1;
		border: 2px solid #ecf0f1;
		color: #000;
		cursor: pointer;

		&:hover {
			border-color: $link-color;
			color: #000;
		}

		&__i {
			position: relative;
			padding: 0 26px 0 73px;
		}

		&__title {
			font-size: 26px;
			line-height: 26px;
			font-weight: bold;
		}

		&__descr {
			display: block;
			margin-top: 3px;
			font-size: 11px;
			line-height: 11px;
			font-weight: 900;
			text-transform: uppercase;
			letter-spacing: 1px;
			color: rgba(0,0,0,.3);
		}

		&__arrow {
			position: absolute;
			top: 43px;
			right: 26px;
			width: 28px;
			height: 23px;
		}

		&__link {
			display: none;
		}

		&__icon {
			position: absolute;
			left: 40px;

			&.--n1 {
				top: 0;
				left: 33px;
				width: 26px;
			}
			&.--n2 {
				top: -4px;
				left: 34px;
				width: 22px;
			}
			&.--n3 {
				top: 4px;
				left: 38px;
				width: 14px;
			}
			&.--n4 {
				top: 1px;
				left: 34px;
				width: 22px;
			}
		}

		@media (--not-palm) {
			lost-column: 1/2;
			height: 330px;
			margin-bottom: 22px;
			display: flex;
			flex-direction: row;
			align-items: center;
			justify-content: space-around;
			text-align: center;

			&.--bordered {
				background: #fff;
			}

			&__i {
				position: relative;
				padding: 0;
			}

			&__title {
				font-size: 36px;
				line-height: 46px;
				padding-left: 56px;
			}

			&__descr {
				margin-top: 15px;
				letter-spacing: 1.5px;
			}

			&__icon {
				&.--n1 {
					top: 3px;
					left: 0;
					width: 32px;
				}
				&.--n2 {
					top: 0;
					left: 0;
					width: 30px;
				}
				&.--n3 {
					top: 3px;
					left: 0;
					width: 20px;
				}
				&.--n4 {
					left: 0;
					top: 0;
					width: 31px;
				}
			}

			&__arrow {
				display: none;
			}

			&__title {
				font-size: 36px;
				line-height: 36px;
			}

			&__link {
				display: block;
				position: absolute;
				left: 0;
				right: 0;
				bottom: 34px;
				font-size: 18px;
				line-height: 22px;
				text-align: center;
				text-decoration: underline;
				font-weight: bold;
				color: $link-color;
				letter-spacing: 0.5px;
			}
		}
	}
	@media (--not-palm) {
		&__title {
			margin-bottom: 45px;
			font-size: 36px;
			line-height: 46px;
		}
	}

}
.faq {
	@media (--lap-and-bigger) {
		margin-bottom: 30px;
		padding-bottom: 30px;
	}

	&__title {
		margin-bottom: 29px;
		font-weight: bold;
		font-size: 36px;
		line-height: 46px;

		@media (--palm) {
			margin-bottom: 14px;
			font-size: 24px;
			line-height: 32px;
		}
	}
	&__columns {
		@media (--lap-and-bigger) {
			lost-utility: clearfix;
		}
	}
	&__column {
		@media (--lap-and-bigger) {
			lost-column: 1/2;
		}
	}
	&__list {
	}
	&__item {
		position: relative;
		margin-bottom: 19px;
		padding-right: $column-size;
		padding-left: 22px;

		@media (--lap) {
			padding-right: 40px;
		}
		@media (--lap-and-bigger) {
			&:last-child {
				margin-bottom: 0;
			}
		}
		@media (--palm) {
			margin-bottom: 15px;
			padding-right: 0;
		}
	}
	&__question {
		font-weight: bold;
		font-size: 15px;
		line-height: 22px;
		transition: all $transition-duration;

		&:before {
			position: absolute;
			top: 8px;
			left: 0;
			display: block;
			width: width("img/arrow-down.svg");
			height: height("img/arrow-down.svg");
			background: svg-load("img/arrow-down.svg", fill: $link-color) 0 0 no-repeat;
			content: "";
			transition: transform $transition-duration;

			.is-expanded & {
				transform: rotate(180deg);
			}
		}
		&:hover:before {
			background-image: svg-load("img/arrow-down.svg", fill: $link-color--hover);
		}
		&:hover {
			color: $link-color--hover;
			cursor: pointer;
		}
	}
	&__answer {
		display: none;
		margin-top: 5px;

		.is-expanded & {
			display: block;
		}
		a {
			color: currentColor;
			text-decoration: underline;

			&:hover {
				color: $link-color--hover;
			}
		}
	}
}
.get {
	text-align: center;

	&__title {
		margin-bottom: 40px;
		font-size: 36px;
		line-height: 46px;
	}

	&__btn {
		margin-bottom: 57px;
	}

	@media (--palm) {
		display: none;
	}
}

.btn {
	display: inline-block;
	padding: 0 30px;
	border-radius: 5px;
	background: $link-color--active;
	text-align: center;
	color: #fff;
	font-weight: bold;
	font-size: 20px;
	line-height: 45px;

	&:hover {
		background: #eb3e21;
		color: #fff;
	}
}

.footer {
	padding: 20px 0 30px;
	border-top: 2px solid #ecf0f1;

	&__links {
		margin-bottom: 5px;
	}

	&__link {
		display: block;
		margin-bottom: 10px;

		&__icon {
			position: relative;
			top: -1px;
			width: 18px;
			margin-right: 4px;
			vertical-align: middle;
		}
	}

	@media (--palm) {
		margin-top: 14px;
	}

	@media (--not-palm) {
		display: flex;
		flex-direction: row;
		justify-content: space-between;

		&__links {
			order: 1;
			text-align: right;
		}

		&__link {
			display: inline;
			margin-left: 35px;
			white-space: nowrap;
		}
	}

	@media (--lap) {
		&__link {

		}
	}
}

.product {
	&__gallery {
		opacity: 0;
		transition: opacity .3s;

		&__i {
			position: relative;
		}

		&.--active {
			opacity: 1;
		}

		@media (--lap) {
			padding-left: 13px;
			padding-right: 13px;
		}
	}

	&__details {
		padding-left: 13px;
		padding-right: 13px;

		@media (--not-palm) {
			padding: 0;
		}
	}

	@media (--desk) {
		display: flex;
		flex-wrap: nowrap;
		max-width: 1242px;
		margin-left: auto;
		margin-right: auto;
		padding-left: 20px;
		padding-right: 20px;
		margin-bottom: 30px;

		&__gallery {
			flex: 0 0 calc(8.33 * 8)%;
			padding-right: $gutter-size;
		}

		&__details {
			flex: 0 0 calc($column-size * 4);
		}
	}
}

.gallery {
	&__item {
		&__img {
			width: 100%;
		}
	}

	&__thumbs {
		display: none;

		@media (--not-palm) {
			display: flex;
			flex-wrap: wrap;
			margin-top: 20px;

			&__item {
				width: 80px;
				height: auto;
				margin-right: 16px;
				margin-bottom: 20px;
				border: 2px solid #fff;
				border-radius: 0;
				background: none;

				&:active {
					outline: none;
				}

				&.tns-nav-active {
					background: none;
					box-shadow: 0 0 0 2px $link-color;
				}

				&__img {
					display: inline;
					width: 100%;
				}
			}
		}
	}

	&__btns {
		position: absolute;
		right: 13px;
		top: 50%;
		margin-top: -10px;

		&__arrow {
			width: 22px;
			height: 20px;
			outline: none;
			user-select: none;
			background: url(img/slider-arrow-mobile.svg) 0 0 no-repeat;

			&.--prev {
				display: none;
			}
		}
	}

	@media (--not-palm) {
		&__btns {
			margin-top: -34px;
			right: 39px;

			&__arrow {
				width: 68px;
				height: 68px;
				background: url(img/slider-arrow.svg) 0 0 no-repeat;
				cursor: pointer;
			}
		}
	}
}

.sticky {
	position: fixed;
	left: 0;
	bottom: 0;
	right: 0;
	padding: 11px 15px;
	border-top: 1px solid #ecf0f1;
	background: #fff;

	@media (--not-palm) {
		display: none;
	}
}

.sticky__btn {
	width: 100%;
}

